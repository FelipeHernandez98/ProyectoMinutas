<div class="container p-4">
    <div class="row ml-auto">

        <div class="col"><h1>Lista minutas</h1></div>                                    
    <div class="row">
        
        <table id="users" class="table table-striped table-dark">
            <thead class="">
                <tr>
                
                <th scope="col">Nombre vigilante</th>
                <th scope="col">Descripcion</th>
                <th scope="col">Fecha</th>
                </tr>
            </thead>
            <tbody>
            {{#each minutas}}
            
                <tr>
                    
                    <td>{{nombre_vigilante}}</td>
                    <td>{{descripcion}}</td>
                    <td>{{fecha}}</td>

                </tr>
             
        {{/each}}
            </tbody>
        </table>
    </div>
</div>